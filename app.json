{
  "expo": {
    "name": "walletpro-mobile",
    "slug": "walletpro-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "account-kit-expo-quickstart",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "EXPO_PUBLIC_ALCHEMY_API_KEY": "ZD_ZC1-j-ke_Fhw9qfjCdo5CyZGsgVfQ",
      "EXPO_PUBLIC_ALCHEMY_POLICY_ID": "",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a5ea2541-4957-4a3a-b99f-f1e6c3d39fbe"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alchemy.accountkitexpoquickstart"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alchemy.accountkitexpoquickstart"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "17.0"
          }
        }
      ],
      [
        "expo-navigation-bar",
        {
          "backgroundColor": "#0f172a",
          "barStyle": "light",
          "borderColor": "#1f2937",
          "visibility": "visible",
          "behavior": "inset-swipe",
          "position": "relative"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
